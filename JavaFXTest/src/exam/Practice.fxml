<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox fx:controller="exam.practice_controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="43.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="42.0" prefWidth="58.0" text="이름 :">
               <opaqueInsets>
                  <Insets top="10.0" />
               </opaqueInsets>
            </Label>
            <TextField fx:id="name" alignment="BOTTOM_CENTER" translateY="10.0" />
         </children>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="42.0" prefWidth="58.0" text="성별 :">
               <opaqueInsets>
                  <Insets top="10.0" />
               </opaqueInsets>
            </Label>
            <RadioButton fx:id="male" mnemonicParsing="false" text="남" translateY="12.0" />
            <RadioButton fx:id="female" mnemonicParsing="false" text="여" translateX="20.0" translateY="12.0" />
         </children>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="42.0" prefWidth="58.0" text="취미 :">
               <opaqueInsets>
                  <Insets top="10.0" />
               </opaqueInsets>
            </Label>
            <CheckBox fx:id="travel" mnemonicParsing="false" text="여행" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="hiking" mnemonicParsing="false" text="등산" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="rdbook" mnemonicParsing="false" text="독서" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="baduk" mnemonicParsing="false" text="바둑" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="changgi" mnemonicParsing="false" text="장기" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="game" mnemonicParsing="false" text="게임" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="tennis" mnemonicParsing="false" text="테니스" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
            <CheckBox fx:id="bdmin" mnemonicParsing="false" text="배드민턴" translateY="12.0">
               <padding>
                  <Insets right="15.0" />
               </padding>
            </CheckBox>
         </children>
      </HBox>
      <HBox prefHeight="41.0" prefWidth="600.0">
         <children>
            <Button fx:id="select" mnemonicParsing="false" text="보기" translateX="15.0" translateY="10.0" onAction="#selectClicked"/>
         </children>
      </HBox>
      <HBox prefHeight="224.0" prefWidth="600.0">
         <children>
            <TextArea fx:id="text" prefHeight="198.0" prefWidth="241.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </TextArea>
         </children>
      </HBox>
   </children>
</VBox>
